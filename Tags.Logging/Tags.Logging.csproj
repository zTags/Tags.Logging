<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  
  <PropertyGroup>
    <PackageId>Tags.Logging</PackageId>
    <Version>1.0.0</Version>
    <Authors>tags</Authors>
    <PackRelease>true</PackRelease>
    <PackageTags>logging;</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryLink>https://github.com/zTags/Tags.Logging</RepositoryLink>
    <Description>
      Very simple logging library 
    </Description>
    <PackageProjectUrl>https://github.com/zTags/Tags.Logging</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>
</Project>
